function caseInSwitch (val) {
    var answer = ``;
    switch (val) {
        case 10:
            answer ="Greater than 10";
            break;
        case (val <= 10):
            answer ="Less than 10";
            break;
    default:
        answer ="Please give a number"
    }
    return answer;
}
console.log(caseInSwitch(10));

/* how can i set up a date that i want (not just the empty date) that can be shown in the console, for 
example var dob = new Date(1995, 11, 24, 00, 00, 00); see page 136 */
var dob = new Date(1995, 11, 24, 00, 00, 00);
console.log(dob.toDateString());
console.log(dob.getDate());
// now it works, wtf


var outerWear = "Tshirt";
function myOutfit(){
    var outerWear = 'sweater';
    return outerWear;
    }
console.log(myOutfit()); // gives sweater
console.log(outerWear); /* by using var to declare the variable in the function, we kept the scope local, so when the 
second console.log for outerWear was made, it referenced the global variable giving Tshirt */

